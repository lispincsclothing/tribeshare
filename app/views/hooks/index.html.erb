<div class="container">


<p id="notice"><%= notice %></p>

<h1>Listing Hooks</h1>

<table>
  <thead>
    <tr>
      <th>Rfid</th>
      <th>Photon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hooks.each do |hook| %>
      <tr>
        <td><%= hook.rfid %></td>
        <td><%= hook.photon %></td>
        <td><%= link_to 'Show', hook %></td>
        <td><%= link_to 'Edit', edit_hook_path(hook) %></td>
        <td><%= link_to 'Destroy', hook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
<div class="container">


<%= link_to 'New Hook', new_hook_path %>

</div>

<p id="notice"><%= notice %></p>

<h1>Listing Campaigns</h1>

<table>
  <thead>
    <tr>
      <th>Header</th>
      <th>Cost</th>
      <th>Title</th>
      <th>Picture1</th>
      <th>Picture2</th>
      <th>Picture1 title</th>
      <th>Picture2 title</th>
      <th>Description</th>
      <th>Expiration</th>
      <th>Minimum contribution</th>
      <th>Rfid</th>
      <th>Owner</th>
      <th>Custodian</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campaigns.each do |campaign| %>
      <tr>
        <td><%= campaign.header %></td>
        <td><%= campaign.cost %></td>
        <td><%= campaign.title %></td>
        <td><%= campaign.picture1 %></td>
        <td><%= campaign.picture2 %></td>
        <td><%= campaign.picture1_title %></td>
        <td><%= campaign.picture2_title %></td>
        <td><%= campaign.description %></td>
        <td><%= campaign.expiration %></td>
        <td><%= campaign.minimum_contribution %></td>
        <td><%= campaign.rfid %></td>
        <td><%= campaign.owner %></td>
        <td><%= campaign.custodian %></td>
        <td><%= link_to 'Show', campaign %></td>
        <td><%= link_to 'Edit', edit_campaign_path(campaign) %></td>
        <td><%= link_to 'Destroy', campaign, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campaign', new_campaign_path %>
